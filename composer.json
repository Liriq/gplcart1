{
  "name": "gplcart/gplcart",
  "type": "project",
  "description": "GPLCart",
  "keywords": [
    "gplcart",
    "ecommerce",
    "shopping cart",
    "opensource shopping cart"
  ],
  "homepage": "http://www.gplcart.com",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Iurii Makukh",
      "email": "gplcart.software@gmail.com",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/gplcart/gplcart/issues"
  },
  "require": {
    "php": ">=5.4.0",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-fileinfo": "*",
    "ext-openssl": "*",
    "ext-spl": "*",
    "assets/jquery": "2.2.4",
    "assets/jquery_ui": "1.12.1",
    "assets/popper": "1.14.3",
    "assets/bootstrap": "4.1.3",
    "assets/font_awesome": "5.2.0",
    "wikimedia/composer-merge-plugin": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "phpunit/dbunit": "^2.0"
  },
  "extra": {
    "merge-plugin": {
      "include": "system/modules/*/composer.json",
      "recurse": true,
      "replace": false,
      "merge-extra": false
    }
  },
  "autoload": {
    "psr-4": {
      "gplcart\\core\\": "system/core/",
      "gplcart\\tests\\unit\\": "tests/unit/"
    }
  },
  "config": {
    "secure-http": false
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "package",
      "packagist.org": false,
      "package": {
        "name": "assets/jquery",
        "version": "2.2.4",
        "dist": {
          "type": "file",
          "url": "https://code.jquery.com/jquery-2.2.4.min.js"
        }
      }
    },
    {
      "type": "package",
      "packagist.org": false,
      "package": {
        "name": "assets/jquery_ui",
        "version": "1.12.1",
        "dist": {
          "type": "zip",
          "url": "https://jqueryui.com/resources/download/jquery-ui-1.12.1.zip"
        }
      }
    },
    {
      "type": "package",
      "packagist.org": false,
      "package": {
        "name": "assets/popper",
        "version": "1.14.3",
        "dist": {
          "type": "zip",
          "url": "https://github.com/FezVrasta/popper.js/archive/v1.14.3.zip"
        }
      }
    },
    {
      "type": "package",
      "packagist.org": false,
      "package": {
        "name": "assets/bootstrap",
        "version": "4.1.3",
        "dist": {
          "type": "zip",
          "url": "https://github.com/twbs/bootstrap/archive/v4.1.3.zip"
        }
      }
    },
    {
      "type": "package",
      "packagist.org": false,
      "package": {
        "name": "assets/font_awesome",
        "version": "5.2.0",
        "dist": {
          "type": "zip",
          "url": "https://github.com/FortAwesome/Font-Awesome/archive/5.2.0.zip"
        }
      }
    }
  ]
}
